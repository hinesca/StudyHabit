<Window x:Class="StudyHabit.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:View="clr-namespace:StudyHabit.View"
        xmlns:ViewModel="clr-namespace:StudyHabit.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="250">
     <Window.Resources>
          <DataTemplate DataType="{x:Type ViewModel:LoginVM}">
               <View:LoginView/>
          </DataTemplate>
     </Window.Resources>
     <Grid>
          <View:LoginView/>
     </Grid>
</Window>
